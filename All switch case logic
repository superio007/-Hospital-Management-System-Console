                                                                // To Remove specific data from Pharmacy Medicine dealer class
                                                                // To take input from the user

                                                                System.out.println("Enter Pharmacy_Medicine_Dealer_name : ");
                                                                String pharmacy_Medicine_dealer_name = sc.nextLine().toLowerCase();

                                                                System.out.println("Enter Pharmacy_Medicine_Dealer_Loc : ");
                                                                String pharmacy_Medicine_dealer_Loc = sc.nextLine().toLowerCase();


                                                                System.out.println("=================================================================");

                                                                Pharmacy_Medicine objectToRemove = null;

// To find data to be deleted
                                                                for (Pharmacy_Medicine Pm : Pharmacy_Medicine_List) {
                                                                    if (Pm.getPharmacy_Medicine_Dealer_name().equalsIgnoreCase(pharmacy_Medicine_dealer_name)
                                                                            && Pm.getPharmacy_Medicine_Dealer_Loc().equalsIgnoreCase(pharmacy_Medicine_dealer_Loc)
                                                                    ) {
                                                                        objectToRemove = Pm;
                                                                        System.out.println("=================================================================");
                                                                        System.out.println("Data Found");
                                                                        System.out.println("=================================================================");
                                                                        break; // Exit the loop once data is found
                                                                    }
                                                                }

// To remove data
                                                                if (objectToRemove != null) {
                                                                    Pharmacy_Medicine_List.remove(objectToRemove);
                                                                    System.out.println("Data for the following Pharmacy Medicine dealer is removed: " + objectToRemove.getPharmacy_Medicine_Dealer_name());
                                                                    // At this point, the object "Pharmacy_Medicine" is no longer accessible,
                                                                    // and the garbage collector will eventually reclaim its memory.
                                                                } else {
                                                                    System.out.println("No matching object found.");
                                                                }
                                                                System.out.println("=================================================================");
                                                                break;

                                                            case 0:
                                                                System.out.println("Exiting the program. Goodbye!");
                                                                break;
                                                            default:
                                                                System.out.println("Invalid choice. Please try again.");
                                                        }
                                                    }while(Pharmacy_Medicine_Dealer !=0);

