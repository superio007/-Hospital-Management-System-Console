//                    To get specific data from Pharmacy Medicine dealer class

//                    To take user input
                                                                System.out.println("Enter Pharmacy_Medicine_Dealer_name : ");
                                                                String pharmacy_Medicine_Dealer_name = sc.nextLine().toLowerCase();

                                                                System.out.println("Enter Pharmacy_Medicine_Dealer_Loc : ");
                                                                String pharmacy_Medicine_Dealer_Loc = sc.nextLine().toLowerCase();

                                                                System.out.println("=================================================================");

                                                                boolean dataFound = false;

// To find specific data in Pharmacy_Medicine_List
                                                                for (Pharmacy_Medicine PM : Pharmacy_Medicine_List) {
                                                                    if (PM.getPharmacy_Medicine_Dealer_name().equalsIgnoreCase(pharmacy_Medicine_Dealer_name)
                                                                            && PM.getPharmacy_Medicine_Dealer_Loc().equalsIgnoreCase(pharmacy_Medicine_Dealer_Loc)
                                                                    ) {
                                                                        PM.toString();
                                                                        System.out.println("=================================================================");
                                                                        System.out.println("Data found for : " + PM.getPharmacy_Medicine_Dealer_name());
                                                                        System.out.println("=================================================================");
                                                                        dataFound = true;
                                                                        break; // Found the matching object, no need to continue the loop.
                                                                    }
                                                                }

                                                                if (!dataFound) {
                                                                    System.out.println("Data has not been found.......");
                                                                    System.out.println("========================================");
                                                                }

                                                                break;

