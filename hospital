                        if(DL.getD_Username().equals(D_username)
                                && DL.getD_Password().equals(D_Password)){
                            System.out.println("Welcome Doctor : "+DL.getDoctor_First_Name());
                            System.out.println("===================================================================");
                            int Docter_Menu_Choice_Log;

                            do{
                                System.out.println("========= Menu =========");
                                System.out.println("1. Add Patient details");
                                System.out.println("2. Display Patient details");
                                System.out.println("3. To get specific data from Patient class");
                                System.out.println("4. To Remove specific data from Patient class");
                                System.out.println("0. Exit");
                                System.out.println("========================");
                                System.out.println("Enter your choice: ");
                                Docter_Menu_Choice_Log = sc.nextInt();
                                sc.nextLine();
                                switch (Docter_Menu_Choice_Log){

                                    case 1:
//                    Add patient details

                                        System.out.println("Enter Patient name :");
                                        String Patient_Name = sc.nextLine();

                                        System.out.println("Enter Patient_gender :");
                                        String Patient_gender = sc.nextLine();

                                        System.out.println("Enter Patient_age :");
                                        double Patient_age = sc.nextDouble();
                                        sc.nextLine();

                                        System.out.println("Enter Patient_suffering :");
                                        String Patient_suffering = sc.nextLine();

                                        System.out.println("Enter Patient_presciption :");
                                        String Patient_presciption = sc.nextLine();

                                        System.out.println("Enter Patient_medical_history :");
                                        String Patient_medical_history = sc.nextLine();

                                        System.out.println("Enter Patient_Contact_details :");
                                        String Patient_Contact_details = sc.nextLine();

                                        System.out.println("Enter Patient_Address :");
                                        String Patient_Address = sc.nextLine();

                                        System.out.println("Enter Patient_weight :");
                                        double Patient_weight = sc.nextDouble();
                                        sc.nextLine();

                                        System.out.println("Enter Patient_allergies :");
                                        String Patient_allergies = sc.nextLine();

                                        System.out.println("====================================================================");


                                        for(Doctor Da : Doctor_Lists){
                                            Da.setDocter_Patient_name(Patient_Name);
                                            Da.setDocter_Patient_gender(Patient_gender);
                                            Da.setDocter_Patient_age(Patient_age);
                                            Da.setDocter_Patient_suffering(Patient_suffering);
                                            Da.setDocter_Patient_presciption(Patient_presciption);
                                            Da.setDocter_Patient_medical_history(Patient_medical_history);
                                            Da.setDocter_Patient_Contact_details(Patient_Contact_details);
                                            Da.setDocter_Patient_Address(Patient_Address);
                                            Da.setDocter_Patient_weight(Patient_weight);
                                            Da.setDocter_Patient_allergies(Patient_allergies);

                                            System.out.println("Data has been added for : "+Da.getDocter_Patient_name());
                                            System.out.println("====================================================================");

                                            break;

                                        }



                                        break;
                                    case 2:
//                    Display patient details

                                        for(Doctor D : Doctor_Lists){
                                            D.toString();
                                            System.out.println("====================================================================");

                                        }

                                        break;
                                    case 3:
//                    To get specific info from patient


                                        System.out.println("Enter Patient name:");
                                        String patient_Name = sc.nextLine().toLowerCase();

                                        System.out.println("Enter Patient gender:");
                                        String patient_gender = sc.nextLine().toLowerCase();

                                        System.out.println("Enter Patient age:");
                                        double patient_age = sc.nextDouble();
                                        sc.nextLine();

                                        System.out.println("================================================================");

                                        boolean dataFound = false;
                                        for (Doctor d : Doctor_Lists) {
                                            if (d.getDocter_Patient_name().equalsIgnoreCase(patient_Name)
                                                    && d.getDocter_Patient_gender().equalsIgnoreCase(patient_gender)
                                                    && d.getDocter_Patient_age() == patient_age
                                            ) {

                                                System.out.println("Data found");
                                                System.out.println("=====================================================");
                                                d.toString();
                                                System.out.println("===================================================");
                                                dataFound = true;
                                                break;
                                            }
                                        }

                                        if (!dataFound) {
                                            System.out.println("Data is not found.");
                                        }
                                        break;
                                    case 4:
//                    to remove specific info from patient

                                        System.out.println("Enter Patient name:");
                                        String patient_name = sc.nextLine().toLowerCase();

                                        System.out.println("Enter Patient gender:");
                                        String patient_Gender = sc.nextLine().toLowerCase();

                                        System.out.println("Enter Patient age:");
                                        double patient_Age = sc.nextDouble();
                                        sc.nextLine();

                                        System.out.println("================================================================");

                                        Doctor ObjToRemove = null;

                                        for(Doctor Ac : Doctor_Lists){
                                            if(Ac.getDocter_Patient_name().equalsIgnoreCase(patient_name)
                                                    && Ac.getDocter_Patient_gender().equalsIgnoreCase(patient_Gender)
                                                    && Ac.getDocter_Patient_age()==patient_Age)
                                            {
                                                System.out.println("Data found : "+Ac.getDocter_Patient_name());
                                                System.out.println("=================================================================");
                                                ObjToRemove = Ac;
                                                break;
                                            }
                                        }

//                   To remove specific data from account class

                                        if (ObjToRemove != null) {
                                            Doctor_Lists.remove(ObjToRemove);
                                            System.out.println("Data for following patient is removed " + ObjToRemove.getDocter_Patient_name());
                                            // At this point, the object " account staff   "  is no longer accessible,
                                            // and the garbage collector will eventually reclaim its memory.
                                        } else {
                                            System.out.println("No matching object found.");
                                        }
                                        System.out.println("=================================================================");
                                        break;

                                    case 0:
                                        System.out.println("Exiting the program. Goodbye!");
                                        break;
                                    default:
                                        System.out.println("Invalid choice. Please try again.");


                                }

                            }while(Docter_Menu_Choice_Log !=0);
                            break;

                        }else{
                            System.out.println("Username or password is wrong ! try again");
                            System.out.println("====================================================");
                            break;
                        }
                    }


                    break;

                case 4:
//                    Doctor sign up

                    System.out.println("Enter First Name : ");
                    String first_name = sc.nextLine();

                    System.out.println("Enter Middle Name : ");
                    String middle_name = sc.nextLine();

                    System.out.println("Enter Last Name : ");
                    String last_name = sc.nextLine();

                    System.out.println("Enter Username : ");
                    String user_name = sc.nextLine();

                    System.out.println("Enter Password : ");
                    String password_use = sc.nextLine();

                    System.out.println("=================================================================");


                    for(Doctor_Login dl : Doctor_Login_List) {
                        dl.setDoctor_First_Name(first_name);
                        dl.setDoctor_Middle_Name(middle_name);
                        dl.setDoctor_Last_Name(last_name);
                        dl.setD_Username(user_name);
                        dl.setD_Password(password_use);

                        System.out.println("Admin Details are created for : " + dl.getDoctor_First_Name());

                        System.out.println("=================================================================");

                        System.out.println("Your username is : " + dl.getD_Username());

                        System.out.println("=================================================================");
                        break;
                    }
                    break;

                case 0:
                    System.out.println("Exiting the program. Goodbye!");
                    break;
                default:
                    System.out.println("Invalid choice. Please try again.");
            }
        }while(Admin_Choice !=0);
    }
}

