//                    to find data into insurance list

//                    to remove data intialize null first

                                                    Insurance toRemove = null;

                                                    for (Insurance in : InsuranceList) {
                                                        if (in.getPatient_name().equalsIgnoreCase(pAtient_name)
                                                                && in.getPatient_gender().equalsIgnoreCase(pAtient_gender)
                                                                && in.getPatient_age() == pAtient_age
                                                                && in.getPatient_disease().equalsIgnoreCase(pAtient_disease)
                                                                && in.getPatient_claimAmount() == pAtient_claimAmount) {
                                                            System.out.println("Data found ...............");
                                                            toRemove = in;
                                                            System.out.println("=================================================================");
                                                            break;
                                                        }
                                                    }

                                                    if (toRemove != null) {
                                                        InsuranceList.remove(toRemove);
                                                        System.out.println("Data has been removed :  " + toRemove.getPatient_name());
                                                        System.out.println("=================================================================");
                                                    } else {
                                                        System.out.println("Data not found .........................");
                                                        System.out.println("=================================================================");
                                                    }


                                                    break;




