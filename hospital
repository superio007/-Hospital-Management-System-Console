//============================================================================================================================
class Insurance extends Departments {
    private String Patient_name;
    private String Patient_gender;
    private int Patient_age;
    private String Patient_disease;
    private double Patient_claimAmount;

    public String getPatient_name() {
        return Patient_name;
    }

    public void setPatient_name(String patient_name) {
        Patient_name = patient_name;
    }

    public String getPatient_gender() {
        return Patient_gender;
    }

    public void setPatient_gender(String patient_gender) {
        Patient_gender = patient_gender;
    }

    public int getPatient_age() {
        return Patient_age;
    }

    public void setPatient_age(int patient_age) {
        Patient_age = patient_age;
    }

    public String getPatient_disease() {
        return Patient_disease;
    }

    public void setPatient_disease(String patient_disease) {
        Patient_disease = patient_disease;
    }

    //    to get Patient_claimAmount
    public double getPatient_claimAmount() {

        return Patient_claimAmount;
    }


    //    //    to set Patient_claimAmount
    public void setPatient_claimAmount(double patient_claimAmount) {

        Patient_claimAmount = patient_claimAmount;
    }


    public void moneyDeduct() {
        // Assuming hospitalInsuranceAmount is always greater than patientClaimAmount
        double deductAmt = getHospital_Insurance_Amount() - getPatient_claimAmount();
        setHospital_Insurance_Amount(deductAmt);
    }

    Insurance(String Patient_name,String Patient_gender,int Patient_age,String Patient_disease,double Patient_claimAmount){
        this.Patient_name = Patient_name;
        this.Patient_gender = Patient_gender;
        this.Patient_age = Patient_age;
        this.Patient_disease = Patient_disease;
        this.Patient_claimAmount = Patient_claimAmount;
//        this.moneyDeduct();
    }

    @Override
    public String toString(){
        System.out.println("Patient_name : "+Patient_name);
        System.out.println("Patient_gender : "+Patient_gender);
        System.out.println("Patient_age : "+Patient_age);
        System.out.println("Patient_disease : "+Patient_disease);
        System.out.println("Patient_claimAmount : "+Patient_claimAmount);
        return "";
    }

}

//=====================================================================================================

class Management_staff extends Departments{
    private String Staff_First_Name;

    private String Staff_Middle_Name;

    private String Staff_Last_Name;
    private String Staff_Dept_name ;
    private double Staff_sal;

    private int Staff_age;
    private String Staff_gender;
    private String Staff_Designation;

    public String getStaff_First_Name() {
        return Staff_First_Name;
    }

    public void setStaff_First_Name(String staff_First_Name) {
        Staff_First_Name = staff_First_Name;
    }

    public String getStaff_Middle_Name() {
        return Staff_Middle_Name;
    }

    public void setStaff_Middle_Name(String staff_Middle_Name) {
        Staff_Middle_Name = staff_Middle_Name;
    }

    public String getStaff_Last_Name() {
        return Staff_Last_Name;
    }

    public void setStaff_Last_Name(String staff_Last_Name) {
        Staff_Last_Name = staff_Last_Name;
    }

    public String getStaff_Dept_name() {
        return Staff_Dept_name;
    }

    public void setStaff_Dept_name(String staff_Dept_name) {
        Staff_Dept_name = staff_Dept_name;
    }

    public double getStaff_sal() {
        return Staff_sal;
    }

    public void setStaff_sal(double staff_sal) {
        Staff_sal = staff_sal;
    }

    public int getStaff_age() {
        return Staff_age;
    }

    public void setStaff_age(int staff_age) {
        Staff_age = staff_age;
    }

    public String getStaff_gender() {
        return Staff_gender;
    }

    public void setStaff_gender(String staff_gender) {
        Staff_gender = staff_gender;
    }

    public String getStaff_Designation() {
        return Staff_Designation;
    }

    public void setStaff_Designation(String staff_Designation) {
        Staff_Designation = staff_Designation;
    }

        Management_staff(){

    }

    @Override
    public String toString(){
        System.out.println("Staff_First_Name : "+Staff_First_Name);
        System.out.println("Staff_Middle_Name : "+Staff_Middle_Name);
        System.out.println("Staff_Last_Name : "+Staff_Last_Name);
        System.out.println("Staff_Dept_name : "+Staff_Dept_name);
        System.out.println("Staff_sal : "+Staff_sal);
        System.out.println("Staff_age : "+Staff_age);
        System.out.println("Staff_gender : "+Staff_gender);
        System.out.println("Staff_spealization : "+Staff_Designation);
        return "";
    }

    Management_staff(String Staff_First_Name,String Staff_Middle_Name,String Staff_Last_Name,String Staff_Dept_name ,double Staff_sal,int Staff_age,String Staff_gender,String Staff_Designation){
        this.Staff_First_Name = Staff_First_Name;
        this.Staff_Middle_Name = Staff_Middle_Name;
        this.Staff_Last_Name = Staff_Last_Name;
        this.Staff_Dept_name = Staff_Dept_name;
        this.Staff_sal = Staff_sal;
        this.Staff_age = Staff_age;
        this.Staff_gender = Staff_gender;
        this.Staff_Designation = Staff_Designation;
    }

