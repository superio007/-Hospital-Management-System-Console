                                                                && Ac.getDept_Degree().toLowerCase().equals(dept_Degree.toLowerCase())
                                                                && Ac.getDept_Exp() == dept_Exp
                                                                && Ac.getDept_Specialization().toLowerCase().equals(dept_Specialization.toLowerCase())) {
                                                            System.out.println("Data found : " + Ac.getPatient_name());
                                                            Ac.toString();
                                                            Account_Found = true;
                                                            System.out.println("=================================================================");
                                                            break;
                                                        }
                                                    }

